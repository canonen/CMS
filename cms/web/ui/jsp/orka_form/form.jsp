:none;	}	.sendBtn {		cursor:pointer;		border:none;	}	select {		background-color: #EFEFDC;	    border: 1px solid #C7C7B7;	    border-radius: 4px 4px 4px 4px;	    color: #B4954D;	    font-family: arial;	    font-size: 12px;	    font-weight: bold;	    height: 30px;	    padding: 4px;	}</style><script type="text/javascript">	$(function(){		$("#city").change(function(){			$.getJSON("city.jsp",{id: $(this).val(), ajax: 'true'}, function(j){				var options = '';				for (var i = 0; i < j.length; i++) {				options += '<option value="' + j[i].optionValue + '">' + j[i].optionDisplay + '</option>';				}				$("#town").html(options);			})				if($("#city").prop("selectedIndex") == 0)			{				$("#cityName").val("YOK");			}			else			{				$("#cityName").val($("#city option:selected").text());						}		})	})function validateForm(){	if($("#contract").is(':checked')){} 	else {alert("Lütfen sözleşmeyi onaylayınız.");return false;}	if($("#city").prop("selectedIndex") == 0)	{		alert("Lütfen il ve ilçe seçiniz.");		return false;	}	if(($("#dobd").prop("selectedIndex") == 0) || ($("#dobm").prop("selectedIndex") == 0) || ($("#doby").prop("selectedIndex") == 0))	{		alert("Lütfen doğum tarihinizi giriniz.");		return false;	}		return true;}function setShop(shop){	document.getElementById("sShop").value = shop;	document.getElementById("shop"+shop).style.background = '#cd9c17';	for(var i=1;i<4;i++)	{		if(i==shop)			document.getElementById("shop"+shop).style.background = '#cd9c17';		else			document.getElementById("shop"+i).style.background = '#EFEFDC';	}}$(document).ready(function(){	$("#form1").validate({		errorPlacement: function(error,element) {        return true;     },	   onfocusout: true,	   rules: {		 firstname: {	       required: true,	       minlength: 3		 },		 lastname: {		       required: true,		       minlength: 2		 },	     email: {	       required: true,	       email: true	     },	     gsmccode : {required:true},	     gsmext : {required:true,number:true},	     gsmno : {required:true,number:true}	   },	   messages: {		 contract : {required:"Sözleşmeyi kabul etmelisiniz."},		 gsmccode : {number:"*"},		 gsmext : {number:"*"},		 gsmno : {number:"*"},		 firstname: {			 required: "*",			 minlength: "*"				 	     },	     lastname: {			 required: "*",			 minlength: "*"				 	     },	     email: {	       required: "*",	       email: "*"	     }	   }	})	<%		if(request.getParameter("send") != null)		{			out.println("alert('Formu doldurduğunuz için teşekkürler ederiz. İndirim kuponunuz en kısa sürede email ile gönderilecektir.');");					}	%>});</script></head><body><div id="wrapper">	<div id="logo"></div>	<div id="main"><%	Map<Integer, String> map = new LinkedHashMap<Integer, String>();	String[] months = {"Ocak","Şubat","Mart","Nisan","Mayıs","Haziran","Temmuz","Ağustos","Eylül","Ekim","Kasım","Aralık"};			for(int i = 0; i < months.length; i++)	{		map.put(new Integer(i)+1, months[i]);			}		Map<Integer, String> cityMap = new LinkedHashMap<Integer, String>();	cityMap.put(1,"ADANA");	cityMap.put(72,"ADIYAMAN");	cityMap.put(60,"AFYON");	cityMap.put(38,"AĞRI");	cityMap.put(57,"AKSARAY");	cityMap.put(74,"AMASYA");	cityMap.put(4,"ANKARA");	cityMap.put(9,"ANTALYA");	cityMap.put(39,"ARDAHAN");	cityMap.put(77,"ARTVİN");	cityMap.put(12,"AYDIN");	cityMap.put(15,"BALIKESİR");	cityMap.put(49,"BARTIN");	cityMap.put(32,"BATMAN");	cityMap.put(40,"BAYBURT");	cityMap.put(23,"BİLECİK");	cityMap.put(33,"BİNGÖL");	cityMap.put(17,"BİTLİS");	cityMap.put(5,"BOLU");	cityMap.put(10,"BURDUR");	cityMap.put(24,"BURSA");	cityMap.put(16,"ÇANAKKALE");	cityMap.put(6,"ÇANKIRI");	cityMap.put(75,"ÇORUM");	cityMap.put(13,"DENİZLİ");	cityMap.put(34,"DİYARBAKIR");	cityMap.put(7,"DÜZCE");	cityMap.put(36,"EDİRNE");	cityMap.put(64,"ELAZIĞ");	cityMap.put(41,"ERZİNCAN");	cityMap.put(42,"ERZURUM");	cityMap.put(61,"ESKİŞEHİR");	cityMap.put(45,